#include <iostream>
#include <string>
#include <ctime>
#include <cstdlib>
#include <vector>


struct Student {
	int id;
	std::string ime;
	std::string prezime;
	char spol;
	int rez_1_kol;
	int rez_2_kol;
	int rez_zavr;
	int ukupni_usp;
};

std::vector <Student> v_stud;
int lokator = 0;

int main()
{
	
}

void unosPodataka() {
	Student s;
	std::cout << "\nUnesite ID - IME - PREZIME - SPOL - REZULTAT 1. KOLOKVIJA - REZULTAT 2. KOLOKVIJA - REZULTAT ZAVRSNOG ISPITA - UKUPNI USPIJEH\n: ";
	std::cin >> s.id >> s.ime >> s.prezime >> s.spol >> s.rez_1_kol;
	std::cin >> s.rez_2_kol >> s.rez_zavr >> s.ukupni_usp;
	v_stud.push_back(s);
	
}
