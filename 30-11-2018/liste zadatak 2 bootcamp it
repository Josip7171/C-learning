#include <iostream>
#include <iomanip>
#include <vector>
#include<list>

int main()
{
	std::list<int> list1;
	int counter = 0;
	int unos = 0;
	for(int i = 0; i < 1;){
		std::cout << "Unesi element u listu ( \"100\" za izlaz) : ";
		std::cin >> unos;
		if (unos > 100) {
			list1.push_back(unos);
		}
		else if (unos < 100) {
			list1.push_front(unos);
		}
		else {
			i++;
		}
		counter++;
	}


	std::cout << "\n";
	for (std::list<int>::iterator i = list1.begin(); i != list1.end(); ++i) {
		std::cout << "  " << *i;
	}

}
